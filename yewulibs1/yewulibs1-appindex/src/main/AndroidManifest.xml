<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.slbappindex">

    <application
        android:allowBackup="true"
        android:label="@string/app_nameappindex"
        android:supportsRtl="true">
        <activity
            android:name="com.example.slbappindex.index.ShouyeActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:exported="false"
            android:theme="@style/AppThemeBase2">
            <!--            android:windowSoftInputMode="stateVisible|adjustPan"-->
            <intent-filter>
                <action android:name="${applicationId}.hs.act.slbapp.shouye" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!--Polyv TEST-->
        <activity
            android:name="com.example.slbappindex.polyv.PolyvActList1"
            android:exported="false">
            <intent-filter>
                <action android:name="${applicationId}.hs.act.slbapp.PolyvActList1" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!--login TEST-->
        <activity
            android:name=".logindemo.LoginActDemo"
            android:exported="false">
            <intent-filter>
                <action android:name="${applicationId}.hs.act.slbapp.LoginActDemo" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".logindemo.AdLoginActDemo"
            android:exported="false">
            <intent-filter>
                <action android:name="${applicationId}.hs.act.slbapp.AdLoginActDemo" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".logindemo.AdLoginImgActDemo"
            android:exported="false"
            android:theme="@style/ad_common_dialogstyle1">
            <intent-filter>
                <action android:name="${applicationId}.hs.act.slbapp.AdLoginImgActDemo" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!--MOBID TEST-->
        <service
            android:name="com.example.slbappindex.services.MOBIDservices"
            android:enabled="true"
            android:exported="false" />
        <service android:name="com.example.slbappindex.services.MOBIDServicesBg" />
    </application>
</manifest>
