<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorE60000">

        <TextView
            android:id="@+id/tv_sz1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="30dp"
            android:text="登录"
            android:textColor="@color/white"
            android:textSize="20mm"
            android:visibility="gone"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/tv_title1111"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginRight="30dp"
            android:padding="20mm"
            android:text="视频会议"
            android:textColor="@color/white"
            android:textSize="18mm"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_sz2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="30dp"
            android:text="设置"
            android:textColor="@color/white"
            android:textSize="20mm"
            android:visibility="gone"
            tools:visibility="visible" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/meetinglayout2"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="0dp"
        android:background="@color/colorE60000"
        android:minHeight="40dp"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/entity_meeting2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <TextView
                android:id="@+id/entity_start_meeting_open2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableTop="@drawable/icon_conf_join"
                android:drawablePadding="14dp"
                android:gravity="center"
                android:text="加入会议"
                android:textColor="@color/white"
                android:textSize="14mm" />

            <TextView
                android:id="@+id/entity_reser_metting_open2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableTop="@drawable/icon_conf_create"
                android:drawablePadding="14dp"
                android:gravity="center"
                android:text="创建会议"
                android:textColor="@color/white"
                android:textSize="14mm" />

            <TextView
                android:id="@+id/entity_enterprise_metting_open2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableTop="@drawable/icon_conf_history"
                android:drawablePadding="14dp"
                android:gravity="center"
                android:text="历史会议"
                android:textColor="@color/white"
                android:textSize="14mm" />
        </LinearLayout>


    </LinearLayout>

    <View
        android:id="@+id/bg_content2"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>