<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" tools:ignore="MissingDefaultResource">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="0mm" android:color="#C50405" />
            <solid android:color="#C50405" />
            <corners android:radius="6mm" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:width="0mm" android:color="#EBEEF5" />
            <solid android:color="#EBEEF5" />
            <corners android:radius="6mm" />
        </shape>
    </item>
</selector>