<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:layout_width="match_parent"
        android:layout_height="100mm"
        android:background="@color/colorE60000" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        android:orientation="vertical">

        <include layout="@layout/activity_co_top2" />

        <View
            android:layout_width="match_parent"
            android:layout_height="10mm"
            android:background="#F1F2F4" />

        <RelativeLayout
            android:id="@+id/rl1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0mm"
            android:background="@color/white">

            <TextView
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12mm"
                android:layout_marginTop="16mm"
                android:layout_marginBottom="16mm"
                android:text="@string/appmy23"
                android:textColor="@color/color333333"
                android:textSize="16mm" />

            <com.geek.libglide47.base.GlideImageView
                android:id="@+id/iv_headportrait"
                android:layout_width="38mm"
                android:layout_height="38mm"
                android:layout_marginLeft="14mm"
                android:layout_marginTop="10dp"
                android:layout_marginRight="12mm"
                android:layout_toLeftOf="@+id/iv1"
                android:scaleType="centerCrop"
                android:src="@drawable/icon_grxx1" />

            <ImageView
                android:id="@+id/iv1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12mm"
                android:src="@drawable/icon_my1" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1mm"
            android:layout_marginLeft="12mm"
            android:layout_marginRight="12mm"
            android:background="@color/colorE5E5E5" />

        <RelativeLayout
            android:id="@+id/rl2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0mm"
            android:background="@color/white">

            <TextView
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12mm"
                android:layout_marginTop="16mm"
                android:layout_marginBottom="16mm"
                android:text="@string/appmy24"
                android:textColor="@color/color333333"
                android:textSize="16mm" />

            <TextView
                android:id="@+id/tv_personalname"
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="12mm"
                android:layout_toLeftOf="@+id/iv2"
                android:text=""
                android:textColor="@color/color999999"
                android:textSize="16mm" />

            <ImageView
                android:id="@+id/iv2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12mm"
                android:src="@drawable/icon_my1"
                android:visibility="invisible" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1mm"
            android:layout_marginLeft="12mm"
            android:layout_marginRight="12mm"
            android:background="@color/colorE5E5E5" />

        <RelativeLayout
            android:id="@+id/rl3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0mm"
            android:background="@color/white">

            <TextView
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12mm"
                android:layout_marginTop="16mm"
                android:layout_marginBottom="16mm"
                android:text="@string/appmy25"
                android:textColor="@color/color333333"
                android:textSize="16mm" />

            <TextView
                android:id="@+id/tv_personalphone"
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="12mm"
                android:layout_toLeftOf="@+id/iv3"
                android:text=""
                android:textColor="@color/color999999"
                android:textSize="16mm" />

            <ImageView
                android:id="@+id/iv3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12mm"
                android:src="@drawable/icon_my1"
                android:visibility="invisible" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1mm"
            android:layout_marginLeft="12mm"
            android:layout_marginRight="12mm"
            android:background="@color/colorE5E5E5" />

        <RelativeLayout
            android:id="@+id/rl4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0mm"
            android:background="@color/white">

            <TextView
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12mm"
                android:layout_marginTop="16mm"
                android:layout_marginBottom="16mm"
                android:text="@string/appmy26"
                android:textColor="@color/color333333"
                android:textSize="16mm" />

            <TextView
                android:id="@+id/tv_personalsex"
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="12mm"
                android:layout_toLeftOf="@+id/iv4"
                android:text=""
                android:textColor="@color/color999999"
                android:textSize="16mm" />

            <ImageView
                android:id="@+id/iv4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12mm"
                android:src="@drawable/icon_my1"
                android:visibility="invisible" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1mm"
            android:layout_marginLeft="12mm"
            android:layout_marginRight="12mm"
            android:background="@color/colorE5E5E5" />

        <RelativeLayout
            android:id="@+id/rl5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0mm"
            android:background="@color/white">

            <TextView
                android:id="@+id/tv_personalautograph_name"
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12mm"
                android:layout_marginTop="16mm"
                android:layout_marginBottom="16mm"
                android:text="@string/appmy27"
                android:textColor="@color/color333333"
                android:textSize="16mm" />

            <TextView
                android:id="@+id/tv_personalautograph"
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12mm"
                android:layout_marginRight="12mm"
                android:layout_toLeftOf="@+id/iv5"
                android:layout_toRightOf="@+id/tv_personalautograph_name"
                android:ellipsize="end"
                android:maxLines="1"
                android:textColor="@color/color999999"
                android:textSize="16mm" />

            <ImageView
                android:id="@+id/iv5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12mm"
                android:src="@drawable/icon_my1" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10mm"
            android:background="@color/white"
            android:visibility="gone">

            <TextView
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12mm"
                android:layout_marginTop="16mm"
                android:layout_marginBottom="16mm"
                android:text="@string/appmy28"
                android:textColor="@color/color333333"
                android:textSize="16mm" />

            <TextView
                android:id="@+id/tv_jtzw6"
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="12mm"
                android:layout_toLeftOf="@+id/iv6"
                android:text="..."
                android:textColor="@color/color999999"
                android:textSize="16mm"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/iv6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12mm"
                android:src="@drawable/icon_my1"
                android:visibility="gone" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1mm"
            android:layout_marginLeft="12mm"
            android:layout_marginRight="12mm"
            android:background="@color/colorE5E5E5"
            android:visibility="gone" />

        <RelativeLayout
            android:id="@+id/rl7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0mm"
            android:background="@color/white"
            android:visibility="gone">

            <TextView
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12mm"
                android:layout_marginTop="16mm"
                android:layout_marginBottom="16mm"
                android:text="@string/appmy29"
                android:textColor="@color/color333333"
                android:textSize="16mm" />

            <TextView
                android:id="@+id/tv_jtzw7"
                style="@style/loginstyle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="12mm"
                android:layout_toLeftOf="@+id/iv7"
                android:text="未设置"
                android:textColor="@color/color999999"
                android:textSize="16mm"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/iv7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12mm"
                android:src="@drawable/icon_my1"
                android:visibility="gone" />
        </RelativeLayout>
    </LinearLayout>
</RelativeLayout>